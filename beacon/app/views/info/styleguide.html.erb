        <span class="nt">&lt;div</span> <span class="na">class=</span><span class="s">&quot;can-toggle can-toggle--size-small&quot;</span><span class="nt">&gt;</span>
          <span class="nt">&lt;input</span> <span class="na">id=</span><span class="s">&quot;can-toggle-demo-3&quot;</span> <span class="na">type=</span><span class="s">&quot;checkbox&quot;</span> <span class="na">checked</span><span class="nt">&gt;</span>
          <span class="nt">&lt;label</span> <span class="na">for=</span><span class="s">&quot;can-toggle-demo-3&quot;</span><span class="nt">&gt;</span>
            <span class="nt">&lt;div</span> <span class="na">class=</span><span class="s">&quot;can-toggle__switch&quot;</span> <span class="na">data-checked=</span><span class="s">&quot;On&quot;</span> <span class="na">data-unchecked=</span><span class="s">&quot;Off&quot;</span><span class="nt">&gt;&lt;/div&gt;</span>
            <span class="nt">&lt;div</span> <span class="na">class=</span><span class="s">&quot;can-toggle__label-text&quot;</span><span class="nt">&gt;</span>I&#39;m a little smaller.<span class="nt">&lt;/div&gt;</span>
