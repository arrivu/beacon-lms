/home/sysadmin/dev/instructure/canvas-lms/spec/lib/user_merge_spec.rb:
   46      it "should move ccs to the new user (but only if they don't already exist)" do
   47        # unconfirmed => active conflict
   48:       user1.communication_channels.create!(:path => 'a@instructure.com')
   49:       user2.communication_channels.create!(:path => 'A@instructure.com') { |cc| cc.workflow_state = 'active' }
   50        # active => unconfirmed conflict
   51:       user1.communication_channels.create!(:path => 'b@instructure.com') { |cc| cc.workflow_state = 'active' }
   52:       user2.communication_channels.create!(:path => 'B@instructure.com')
   53        # active => active conflict
   54:       user1.communication_channels.create!(:path => 'c@instructure.com') { |cc| cc.workflow_state = 'active' }
   55:       user2.communication_channels.create!(:path => 'C@instructure.com') { |cc| cc.workflow_state = 'active' }
   56        # unconfirmed => unconfirmed conflict
   57:       user1.communication_channels.create!(:path => 'd@instructure.com')
   58:       user2.communication_channels.create!(:path => 'D@instructure.com')
   59        # retired => unconfirmed conflict
   60:       user1.communication_channels.create!(:path => 'e@instructure.com') { |cc| cc.workflow_state = 'retired' }
   61:       user2.communication_channels.create!(:path => 'E@instructure.com')
   62        # unconfirmed => retired conflict
   63:       user1.communication_channels.create!(:path => 'f@instructure.com')
   64:       user2.communication_channels.create!(:path => 'F@instructure.com') { |cc| cc.workflow_state = 'retired' }
   65        # retired => active conflict
   66:       user1.communication_channels.create!(:path => 'g@instructure.com') { |cc| cc.workflow_state = 'retired' }
   67:       user2.communication_channels.create!(:path => 'G@instructure.com') { |cc| cc.workflow_state = 'active' }
   68        # active => retired conflict
   69:       user1.communication_channels.create!(:path => 'h@instructure.com') { |cc| cc.workflow_state = 'active' }
   70:       user2.communication_channels.create!(:path => 'H@instructure.com') { |cc| cc.workflow_state = 'retired' }
   71        # retired => retired conflict
   72:       user1.communication_channels.create!(:path => 'i@instructure.com') { |cc| cc.workflow_state = 'retired' }
   73:       user2.communication_channels.create!(:path => 'I@instructure.com') { |cc| cc.workflow_state = 'retired' }
   74        # <nothing> => active
   75:       user2.communication_channels.create!(:path => 'j@instructure.com') { |cc| cc.workflow_state = 'active' }
   76        # active => <nothing>
   77:       user1.communication_channels.create!(:path => 'k@instructure.com') { |cc| cc.workflow_state = 'active' }
   78        # <nothing> => unconfirmed
   79:       user2.communication_channels.create!(:path => 'l@instructure.com')
   80        # unconfirmed => <nothing>
   81:       user1.communication_channels.create!(:path => 'm@instructure.com')
   82        # <nothing> => retired
   83:       user2.communication_channels.create!(:path => 'n@instructure.com') { |cc| cc.workflow_state = 'retired' }
   84        # retired => <nothing>
   85:       user1.communication_channels.create!(:path => 'o@instructure.com') { |cc| cc.workflow_state = 'retired' }
   86  
   87        UserMerge.from(user1).into(user2)
   ..
   89        user2.reload
   90        user2.communication_channels.map { |cc| [cc.path, cc.workflow_state] }.sort.should == [
   91:           ['A@instructure.com', 'active'],
   92:           ['B@instructure.com', 'retired'],
   93:           ['C@instructure.com', 'active'],
   94:           ['D@instructure.com', 'unconfirmed'],
   95:           ['E@instructure.com', 'unconfirmed'],
   96:           ['F@instructure.com', 'retired'],
